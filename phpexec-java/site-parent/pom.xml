<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    
    <properties>
        <phpmavenTagName>${project.version}</phpmavenTagName>
        <phpmavenRootScmUrl>scm:git:git@github.com:php-maven/phpexec-java.git</phpmavenRootScmUrl>
        <phpmavenRootScmUrlWeb>http://www.github.com/php-maven/phpexec-java</phpmavenRootScmUrlWeb>
    </properties>

    <parent>
		<groupId>org.phpmaven.build</groupId>
		<artifactId>common-parent</artifactId>
		<version>1</version>
		<relativePath>../../phpmaven-common-parent/pom.xml</relativePath>
    </parent>

    <groupId>org.phpmaven.build.phpexec-java</groupId>
    <artifactId>generic-site-parent</artifactId>
    <version>0.1.10-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>site-parent:${project.version}</name>

    
    <distributionManagement>
        <site>
            <id>upload-${phpmavenSitesId}</id>
            <name>${phpmavenSitesName}</name>
            <url>${phpmavenSitesUploadUrl}/var/sites-aggregator</url>
        </site>
    </distributionManagement>

    <description>A public API for executing php from java.</description>
</project>